-- Table: "Cliente"

-- DROP TABLE "Cliente";

CREATE TABLE "Cliente"
(
  cod serial NOT NULL,
  nome character varying(20) NOT NULL,
  "end" character varying(50) NOT NULL,
  fone_1 character(8) NOT NULL,
  fone_2 character(8),
  obs character(50),
  CONSTRAINT pk_codcliente PRIMARY KEY (cod )
)
WITH (
  OIDS=FALSE
);
ALTER TABLE "Cliente"
  OWNER TO postgres;
